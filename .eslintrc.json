{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["standard"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"quotes": [2, "double"],
		"semi": [2, "always"],
		"no-console": "off",
		"no-tabs": "off",
		"indent": "off",
		"space-before-function-paren": "off",
		"comma-dangle": "off"
	}
}
